<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kjplusapp">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.InformationActivity" />
        <activity android:name=".activity.HuanzheXinxiActivity" />
        <activity
            android:name=".activity.XueyaActivity"
            android:configChanges="orientation|keyboardHidden" />
        <activity android:name=".activity.BluetoothConnActivity" />

        <service android:name="com.iknet.iknetbluetoothlibrary.BluetoothService" />

        <activity android:name=".activity.SignActivity" />
        <activity android:name=".activity.ZhenliaoServiceActivity" />
        <activity android:name=".activity.Information2Activity" />
        <activity android:name=".activity.VisitActivity" />
        <activity android:name=".activity.DiabetesActivity"/>
    </application>

</manifest>